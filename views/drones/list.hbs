<h2>Available drones</h2>

{{#each drones}}
<div>
    <h3>{{name}}</h3>
    <p>Propeller:{{propellers}}</p>
    <p>Speed:{{maxSpeed}}</p>
    <a href="/drones/{{_id}}/edit">Edit</a>
    <form action="/drones/{{_id}}/delete" method="post">
        <button type="submit">Delete Drone</button>
    </form>
</div>
{{/each}}